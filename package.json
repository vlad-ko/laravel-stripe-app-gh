{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
        "@babel/preset-env": "^7.20.2",
        "@vue/test-utils": "^1.3.3",
        "axios": "^0.21.4",
        "babel-core": "^7.0.0-0",
        "babel-jest": "^27.5.1",
        "jest": "^27.5.1",
        "jsdom-global": "^3.0.2",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.21",
        "postcss": "^8.4.20",
        "vue": "^2.7.14",
        "vue-jest": "^3.0.7",
        "vue-template-compiler": "^2.7.14"
    },
    "jest": {
        "transform": {
            "^.+\\.vue$": "vue-jest",
            "^.+\\.js$": "babel-jest"
        },
        "testEnvironment": "jsdom",
        "collectCoverage": true,
        "coverageReporters": [
            "json",
            "html"
        ]
    }
}

        "axios": "^1.7.2",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom-global": "^3.0.2",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.21",
        "postcss": "^8.4.20",
        "vue-jest": "^3.0.7",
        "vue-template-compiler": "^2.7.16"
    },
    "jest": {
